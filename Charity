create table Volunteers
(
 Volunteer_id int primary key,
 Volunteer_name varchar(30) not null,
 Email varchar(50) not null,
 Vol_mobile varchar(10) not null,
 Vol_address varchar(20) not null,
 Vol_age int not null check(Vol_age<=60),
 Vol_gender char(1) not null check(Vol_gender='F' or Vol_gender='M'),
)
