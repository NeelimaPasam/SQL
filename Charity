create table Volunteer_Training
(
  Training_id int primary key,
  Volunteer_id int foreign key references Volunteers(Volunteer_id),
  Training_name varchar(50) not null,
  Training_date date not null,
  Training_duration int not null,
  Training_status varchar(50) not null,
)
