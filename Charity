create table Coordinator
(
  Coordinator_id int primary key,
  Coordinator_name varchar(50) not null,
  Project_id int not null foreign key references Project(project_id),
  Mobile_no varchar(10) not null,
  Email_id varchar(50) not null,
  Age  int not null check(Age>=22 and Age<=50),
  Gender varchar(1) not null check(Gender='F' or Gender='M'),
  Address varchar(100) not null,
)
