create table Donations
(
  Donation_id    int   primary key,
  Donor_id       int   not null foreign key references Donors(Donor_id),
  Donor_name     varchar(50) not null,
  Amount         money not null,
  Donation_date  date  not null,
  Payment_method varchar(50)  not null check(Payment_method='Debit card'or Payment_method='Credit card'or Payment_method='Net Banking'or Payment_method='UPI'),
  Transaction_id int   not null,
)
